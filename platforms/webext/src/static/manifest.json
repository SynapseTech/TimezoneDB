{
  "name": "TimezoneDB",
  "version": "0.0.1",
  "description": "",
  "manifest_version": 2,
  "permissions": ["storage", "<all_urls>"],
  "icons": {
    "16": "logo-16.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content-script.js"]
    }
  ]
}
